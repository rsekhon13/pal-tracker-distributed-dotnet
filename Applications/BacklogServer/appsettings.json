{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "REGISTRATION_SERVER_ENDPOINT": "http://registration-service/",
    "eureka": {
        "client": {
            "serviceUrl": "http://localhost:8761/eureka/",
            "shouldRegisterWithEureka": false,
            "shouldFetchRegistry": true
        }
    },
    "hystrix": {
        "command": {
            "default": {
                "execution": {
                    "isolation": {
                        "thread": {
                            "timeoutInMilliseconds": 5000
                        }
                    }
                }
            }
        }
    }
}